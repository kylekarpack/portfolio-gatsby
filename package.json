{
  "name": "kyle-karpack-portfolio",
  "description": "The portfolio website of Kyle Karpack",
  "version": "1.0.0",
  "private": true,
  "author": "Kyle Karpack",
  "scripts": {
    "start": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "dev": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop -o",
    "build": "gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier **/*.{js,jsx} --ignore-path .gitignore --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^4.4.0",
    "gatsby-image": "^3.11.0",
    "gatsby-omni-font-loader": "^1.3.3",
    "gatsby-plugin-catch-links": "^4.1.0",
    "gatsby-plugin-google-analytics": "^4.1.0",
    "gatsby-plugin-image": "^2.1.0",
    "gatsby-plugin-lodash": "^5.1.0",
    "gatsby-plugin-manifest": "^4.1.0",
    "gatsby-plugin-mdx": "^3.1.1",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-offline": "^5.1.1",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sharp": "^4.1.1",
    "gatsby-plugin-sitemap": "^5.1.1",
    "gatsby-plugin-styled-components": "^5.1.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.1.1",
    "gatsby-source-drive": "^0.0.2",
    "gatsby-source-filesystem": "^4.1.0",
    "gatsby-transformer-remark": "^5.1.1",
    "gatsby-transformer-sharp": "^4.1.0",
    "get-image-colors": "^4.0.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.6.0",
    "polished": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-github-graph": "^1.5.0",
    "react-goodreads-shelf": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-map-gl": "^6.1.17",
    "react-spring": "^8.0.27",
    "snyk": "^1.460.0",
    "styled-components": "^5.3.3",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "gatsby-cypress": "^2.1.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.0"
  },
  "keywords": [
    "karpack",
    "portfolio",
    "gatsby"
  ],
  "repository": {
    "url": "https://github.com/kylekarpack/gatsby-portfolio",
    "type": "git"
  },
  "homepage": "https://kylekarpack.com",
  "license": "MIT",
  "main": "n/a",
  "snyk": true
}
