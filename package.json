{
  "name": "kyle-karpack-portfolio",
  "description": "The portfolio website of Kyle Karpack",
  "version": "1.0.0",
  "private": true,
  "author": "Kyle Karpack",
  "scripts": {
    "start": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "dev": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop -o",
    "build": "gatsby build --log-pages",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier \"**/*.{md,mdx} \" --write",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "@fs/gatsby-plugin-drive": "^0.2.1",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.7",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.32.8",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-catch-links": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.8.0",
    "gatsby-plugin-lodash": "^3.7.0",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-mdx": "^1.2.26",
    "gatsby-plugin-netlify": "^2.8.0",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.8.1",
    "gatsby-remark-responsive-iframe": "^2.8.0",
    "gatsby-source-drive": "^0.0.2",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-source-google-docs": "^2.0.0-beta.44",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "get-image-colors": "^2.0.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^1.11.0",
    "polished": "^3.6.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-github-graph": "1.3.0",
    "react-goodreads-shelf": "^1.2.1",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.10.0",
    "react-map-gl": "^5.3.5",
    "react-spring": "^8.0.27",
    "snyk": "^1.460.0",
    "styled-components": "^4.4.1",
    "styled-theming": "^2.2.0"
  },
  "keywords": [
    "karpack",
    "portfolio",
    "gatsby"
  ],
  "repository": {
    "url": "https://github.com/kylekarpack/gatsby-portfolio",
    "type": "git"
  },
  "homepage": "https://kylekarpack.com",
  "license": "MIT",
  "main": "n/a",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "gatsby-cypress": "^0.1.9",
    "prettier": "^1.17.0",
    "start-server-and-test": "^1.9.0"
  },
  "snyk": true
}
